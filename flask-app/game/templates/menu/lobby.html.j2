{% extends 'base.html.j2' %}

{% block scripts %}
<script type="text/javascript" charset="utf-8">
    var socket = io();
    socket.on('connect', function() {
        socket.emit('join', {
          room: '{{ room }}'
        });
    });
</script>
{% endblock %}
{% block header %}
  <h1>{% block title %}Lobby{% endblock %}</h1>
{% endblock %}

{% block content %}
  <h2>lobby</h2>
{% endblock %}
